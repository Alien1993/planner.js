<div class="js-header row">
    {{> header}}
</div>
<div class="js-planner-body row">
    <div class="col-xs-1 planner-timeslots">
        {{#each rows}}
            {{> timeslot}}
        {{/each}}
    </div>
    <div class="col-xs-11 planner-columns">
        <div class="planner-columns-overflow">
            {{#each columns}}
                {{> column}}
            {{/each}}
        </div>
    </div>
</div>
